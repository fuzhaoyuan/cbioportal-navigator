{
  "name": "cbioportal-navigator",
  "version": "1.0.0",
  "description": "MCP server that navigates users to the right cBioPortal pages by resolving natural language queries into URLs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cbioportal-navigator": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "cbioportal",
    "mcp",
    "model-context-protocol",
    "navigator",
    "url-builder",
    "query-resolver"
  ],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cbioportal-ts-api-client": "^0.9.80"
  },
  "devDependencies": {
    "@types/node": "^18.19.130",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "typescript": "^4.0.3"
  }
}
